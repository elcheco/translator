{
  "name": "elcheco/translator",
  "description": "Translation system using neon files, supports plurals and string replacements, fallback language. Extension to Nette Framework ^2.4",
  "type": "library",
  "license": ["BSD-3-Clause"],
  "authors": [
    {
      "name": "Miroslav Koula",
      "email": "mkoula@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "ext-intl": "*",
    "ext-tokenizer": "*",
    "nette/di": "^2.4",
    "nette/neon": "v3.0.0-beta1",
    "nette/safe-stream": "^2.3",
    "nette/utils": "^2.5",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "nette/tester": "^2.0",
    "mockery/mockery": "^1.0@dev",
    "phpstan/phpstan": "^0.9.0@dev"
  },
  "suggest": {
    "monolog/monolog": "To log translation errors."
  },
  "autoload": {
    "psr-4": {
      "ElCheco\\": [
        "src"
      ]
    }
  }
}
